{
    "name": "ourserverdev/send-grid-mailer-bundle",
    "description": "An opensource bundle that integrates SendGrid API to send emails via their mailing service",
    "license": "MIT",
    "authors": [
        {
            "name": "Reynald Mandel",
            "email": "reynaldm@theodo.fr"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "url": "git@github.com:theodo/sendgrid-php.git",
            "type": "vcs"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.1",
        "symfony/options-resolver": "~2.1",
        "sendgrid/sendgrid": "~3.2.0"
    },
    "autoload": {
        "psr-0": { "Theodo\\SendGridMailerBundle": "" }
    },
    "target-dir": "Theodo/SendGridMailerBundle"
}
